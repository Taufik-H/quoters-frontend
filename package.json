{
  "name": "quoters-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "concurrently \"next dev --turbo\" \"tailwindcss --input ./styles/tailwind.css --output ./styles/globals.css --watch\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "lint": "next lint",
    "generate:icon": "npx @svgr/cli --out-dir svg/component --index-template svg/svgr-cli-template.js svg --typescript"
  },
  "dependencies": {
    "next": "13.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.7.0",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.0.0",
    "@svgr/cli": "^6.5.0",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.5.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  }
}
